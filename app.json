{
  "expo": {
    "name": "Smpl Journal",
    "slug": "daily-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Time and money are the most important resources in our lives. This app that allows you to be mindful in how you  are spending both in the pursuit of your goals.",
    "icon": "./assets/smpljournal.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android", "web"],
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sarathk82.smpljournal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sarathk82.smpljournal"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "owner": "sarathk82",
    "extra": {
      "eas": {
        "projectId": "fb0afca1-7b22-4e29-997d-817dd76b35a1"
      }
    }
  }
}
